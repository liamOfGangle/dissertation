<?xml version="1.0"?>
<robot name="simple_eva" xmlns:xacro="http://www.ros.org/wiki/xacro">
	
	<!-- 1st link -->
	<link name="link1">
		<pose>0 0 0 0 0 0</pose>
    	<inertial>
            <mass value="2.01"/>
            <inertia ixx="0.00796" iyy="0.0042" izz="0.00796" 
					 ixy="0" ixz="0" iyz="0"/>
        </inertial> 
		
		<visual name="visual_link1">
			<geometry>
				<box size="0.112 0.112 0.187"/>
			</geometry>
		</visual>
		
		<collision name="collision_link1">
			<geometry>
				<box size ="0.112 0.112 0.187"/>
			</geometry>
		</collision>
	</link>
	
	<!-- 1st joint -->
	<joint name="joint1" type="revolute">
		<parent link="link1"/>
		<child link="link2"/>
		<limit lower="-3.12414" upper="3.12414"
			   effort="30" velocity="1.0"/>
		<origin xyz="0 0 0.0985" rpy=" 0 0 0"/>
	</joint>
	
	<!-- 2nd link -->
	<link name="link2">
		<pose> 0 0 0 0 0 0</pose>
		<inertial>
			<mass value="1.03"/>
			<inertia ixx="0.000977" iyy="0.000977" izz="0.000404"
					 ixy="0" ixz="0" iyz="0"/>
		</inertial>
		
		<visual name="visual_link2">
			<geometry>
				<cylinder length="0.096" radius="0.028"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0.028 0.048"/>
		</visual>
		
		<collision name="collision_link2">
			<geometry>
				<cylinder length="0.096" radius="0.028"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0.028 0.187"/>
		</collision>
	</link>
		
				
